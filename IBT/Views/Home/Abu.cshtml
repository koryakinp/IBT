                <p><localize>AbutDrainageTitle</localize></p>
                <ul>
                    <li><localize>AbutDrainageValue1</localize></li>
                    <li><localize>AbutDrainageValue2</localize></li>
                    <li><localize>AbutDrainageValue3</localize></li>
                    <li><localize html="true">AbutDrainageValue4</localize></li>
                    <li><localize>AbutDrainageValue5</localize></li>
                </ul>
