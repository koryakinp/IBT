            <li class="counter-2"><localize>AbuElectricityValue2</localize></li>
            <li class="counter-3"><localize>AbuElectricityValue3</localize></li>
            <li class="counter-4"><localize html="true">AbuElectricityValue4</localize></li>
            <li class="counter-5"><localize>AbuElectricityValue5</localize></li>
