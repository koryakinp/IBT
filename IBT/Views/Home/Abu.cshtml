                        </localize>
                    </li>
                </ul>
            </li>
        </ul>
        <p>
            <strong><localize>AbuElectricalConsumptionTitle</localize></strong>
        </p>
        <ul>
            <li>
                <localize>
                    <parameter>@Model.DefaultPower</parameter>
                    <parameter>@Model.FanPower</parameter>
                    AbuElectricalConsumptionValue1
                </localize>
            </li>
            <li>
                <localize html="true">
                    <parameter>@Model.Humidity</parameter>
