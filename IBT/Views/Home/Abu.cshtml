                <ul>
                    <li><localize>AbuCompressedAirValue2.2</localize></li>
                    <li><localize html="true">AbuCompressedAirValue2.3</localize></li>
                    <li><localize>AbuCompressedAirValue2.4</localize></li>
                </ul>
            </li>
            <li class="counter-3"><localize>AbuCompressedAirValue3</localize></li>
            <li class="counter-4"><localize>AbuCompressedAirValue4</localize></li>
            <li class="counter-5"><localize>AbuCompressedAirValue5</localize></li>
        </ol>
        <p><strong><localize>AbuVentilationTitle</localize></strong></p>
        <ol>
            <li>
                <localize>AbuVentilationValue1</localize>
                <p><localize html="true">AbuVentilationValue1.1</localize></p>
                <p><localize html="true">AbuVentilationValue1.2</localize></p>
                <p><localize>AbuVentilationValue1.3</localize></p>
            </li>
            <li>
                <localize>
                    <parameter>@Model.Ventilator</parameter>
                    AbuVentilationValue2
                </localize>
                <ul>
                    <li>
                        <localize>
                            <parameter>@Model.VentilatorEngine</parameter>
