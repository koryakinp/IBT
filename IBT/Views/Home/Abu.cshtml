@using Microsoft.AspNetCore.Mvc.Localization
@using IBT
@model IBT.Models.Abu
@inject IHtmlLocalizer<SharedResource> SharedLocalizer

@{
    ViewData["Title"] = SharedLocalizer.GetString("Abu", Model.Model);
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .download-icon {
        margin-left:10px;
        color:black;
        border-bottom:none;
    }

    .counter-2:before {
        content: "2.";
    }

    .counter-3:before {
        content: "3.";
    }

    .counter-4:before {
        content: "4.";
    }

    .counter-5:before {
        content: "5.";
    }

    .counter-6:before {
        content: "6.";
    }

    .counter-7:before {
        content: "7.";
    }

    .page-title:after {
        width: 100% !important;
    }
</style>

<div class="breadcrumbs">
    <ul class="breadcrumbs__inner">
        <li class="breadcrumbs__item"><a href="/"><localize localizer="SharedLocalizer">MainPage</localize></a></li>
        <li class="breadcrumbs__item"><a href="/Engineering"><localize localizer="SharedLocalizer">ForDesigners</localize></a></li>
        <li class="breadcrumbs__item breadcrumbs__item--active">
            <span>
                <localize localizer="SharedLocalizer">
                    <parameter>@Model.Model</parameter>
                    Abu
                </localize>
            </span>
        </li>
    </ul>
</div>
<div class="content white-text">
    <div class="container">
        <h1>
            <span class="page-title">
                <localize localizer="SharedLocalizer">
                    <parameter>@Model.Model</parameter>
                    Abu
                </localize>
                <a href="/File/Abu/@Model.Model" class="download-icon">
                    <span>
                        <i class="fas fa-file-download"></i>
                    </span>
                </a>
            </span>
        </h1>

        <img class="pull-right" src="@Model.ImageUrl" alt="">

        <div class="page-sub-title">
            <p>
                <localize>
                    <parameter>@Model.Model</parameter>
                    AbuSpecTitle
                </localize>
            </p>
            <p>
                <localize html="true">
                    <parameter>@Model.Model</parameter>
                    <parameter>@Model.Model</parameter>
                    AbuTitle
                </localize>
            </p>
        </div>
        <p>
            <strong>
                <localize>
                    <parameter>@Model.Model</parameter>
                    AbuDimensions
                </localize>
            </strong>
        </p>
        <ul>
            <li>
                <localize>AbuPhenolEfficiencyTitle</localize>
                <ul>
                    <li>
                        <localize html="true">
                            <parameter>95%</parameter>
                            <parameter>>20</parameter>
                            AbuPhenolEfficiencyValue
                        </localize>
                    </li>
                    <li>
                        <localize html="true">
                            <parameter>85%</parameter>
                            <parameter>10-20</parameter>
                            AbuPhenolEfficiencyValue
                        </localize>
                    </li>
                    <li>
                        <localize html="true">
                            <parameter>80%</parameter>
                            <parameter>6-10</parameter>
                            AbuPhenolEfficiencyValue
                        </localize>
                    </li>
                    <li>
                        <localize html="true">
                            <parameter>70%</parameter>
                            <parameter>4-6</parameter>
                            AbuPhenolEfficiencyValue
                        </localize>
                    </li>
                </ul>
            </li>
            <li>
                <localize>AbuTriethylamineEfficiencyValue1</localize>
            </li>
            <li>
                <localize html="true">
                    AbuTriethylamineEfficiencyTitle
                </localize>
                <ul>
                    <li>
                        <localize>AbuTriethylamineEfficiencyValue2</localize>
                    </li>
                    <li>
                        <localize>AbuTriethylamineEfficiencyValue3</localize>
                    </li>
                    <li>
                        <localize html="true">AbuTriethylamineEfficiencyValue4</localize>
                    </li>
                    <li>
                        <localize>
                            <parameter>@Model.Size.Length</parameter>
