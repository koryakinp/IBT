            <li class="counter-5"><localize>AbuInstallationValue5</localize></li>
            <li class="counter-6"><localize html="true">AbuInstallationValue6</localize></li>
            <li class="counter-7"><localize html="true">AbuInstallationValue7</localize></li>
        </ol>
        <p><strong><localize>AbuElectricityTitle</localize></strong></p>
        <ol>
            <li>
                <localize>AbuElectricityValue1</localize>
                <ul>
                    <li>
                        <localize>
                            <parameter>@Model.PowerConsumption1</parameter>
